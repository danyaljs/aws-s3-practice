    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "esbuild": "^0.17.19",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "prettier": "^3.2.5",
    "serverless": "^3.34.0",
    "serverless-esbuild": "^1.46.0",
    "serverless-offline": "^12.0.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
